<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".EditUserInfo">

    <TextView
            android:text="@string/first_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="16dp"
            tools:layout_editor_absoluteX="16dp" android:id="@+id/firstNameLabelTextView"/>
    <EditText
            android:layout_width="257dp"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="117dp"
            tools:layout_editor_absoluteX="33dp" android:id="@+id/lastNameTextEdit" android:hint="Enter last name"
            android:inputType="none"/>
    <EditText
            android:layout_width="255dp"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="35dp"
            tools:layout_editor_absoluteX="33dp" android:id="@+id/firstNameTextEdit" android:hint="Enter first name"
            android:inputType="none"/>
    <TextView
            android:text="@string/last_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="98dp"
            tools:layout_editor_absoluteX="16dp" android:id="@+id/lastNameLabelTextView"/>
    <Button
            android:text="Save"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="426dp"
            tools:layout_editor_absoluteX="105dp" android:id="@+id/saveButton"/>
</android.support.constraint.ConstraintLayout>